spinInfJump.TextColor3 = Color3.fromRGB(13, 89, 231)
spinInfJump.BorderSizePixel = 0

local spinCorner = Instance.new("UICorner", spinInfJump)
local spinStroke = Instance.new("UIStroke", spinInfJump)
spinStroke.Thickness = 2.8
