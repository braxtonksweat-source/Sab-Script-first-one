-- LocalScript: Fully Local GUI Creation

local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Create ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "BK_Hub_GUI"
screenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
screenGui.Parent = playerGui

-- ===== BK's Hub =====
local bkSHub = Instance.new("Frame")
bkSHub.Name = "BK's Hub"
bkSHub.Size = UDim2.fromScale(0.1677, 0.4557)
bkSHub.Position = UDim2.fromScale(0.4516, 0.6203)
bkSHub.AnchorPoint = Vector2.one * 0.5
bkSHub.BackgroundColor3 = Color3.fromRGB(13, 13, 13)
bkSHub.BorderSizePixel = 0
bkSHub.Visible = false
bkSHub.Parent = screenGui

-- UI Constraints & Effects
local bkHubAspect = Instance.new("UIAspectRatioConstraint", bkSHub)
bkHubAspect.AspectRatio = 0.6899

local bkHubCorner = Instance.new("UICorner", bkSHub)
bkHubCorner.CornerRadius = UDim.new(0.05, 0)

local bkHubStroke = Instance.new("UIStroke", bkSHub)
bkHubStroke.Thickness = 7.3
bkHubStroke.Color = Color3.fromRGB(255, 255, 255)

local bkHubGradient = Instance.new("UIGradient", bkHubStroke)
bkHubGradient.Color = ColorSequence.new(Color3.fromRGB(1, 1, 154), Color3.fromRGB(0, 89, 255))

-- TopBar
local topBar = Instance.new("Frame", bkSHub)
topBar.Name = "TopBar"
topBar.Size = UDim2.fromScale(1, 1.0025)
topBar.Position = UDim2.fromScale(0, -0.0026)
topBar.BackgroundTransparency = 1
topBar.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
topBar.BorderSizePixel = 0

local contentFrame = Instance.new("Frame", topBar)
contentFrame.Name = "ContentFrame"
contentFrame.Size = UDim2.fromScale(1, 1)
contentFrame.BackgroundTransparency = 1

-- Hub Label
local bkSHubLabel = Instance.new("TextLabel", bkSHub)
