-- ===== Toggle Button =====
local toggle = Instance.new("TextButton", screenGui)
toggle.Name = "Toggle"
toggle.Text = "Toggle"
toggle.TextScaled = true
toggle.TextWrapped = true
toggle.AnchorPoint = Vector2.one * 0.5
toggle.Position = UDim2.fromScale(0.0645, 0.1317)
toggle.Size = UDim2.fromScale(0.0999, 0.0949)
toggle.BackgroundColor3 = Color3.fromRGB(29, 29, 29)
toggle.TextColor3 = Color3.fromRGB(1, 9, 162)
toggle.BorderSizePixel = 0

local toggleCorner = Instance.new("UICorner", toggle)
local toggleStroke = Instance.new("UIStroke", toggle)
toggleStroke.Thickness = 4.8
toggleStroke.Color = Color3.fromRGB(255, 255, 255)

-- Toggle functionality
toggle.MouseButton1Click:Connect(function()
	bkSHub.Visible = not bkSHub.Visible
	bkSBooster.Visible = not bkSBooster.Visible
end)
