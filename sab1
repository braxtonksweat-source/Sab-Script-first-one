bkSHubLabel.Position = UDim2.fromScale(0.5205, 0.0829)
bkSHubLabel.Font = Enum.Font.SourceSansBold
bkSHubLabel.TextColor3 = Color3.fromRGB(16, 107, 199)
bkSHubLabel.BackgroundTransparency = 1

local hubLabelConstraint = Instance.new("UITextSizeConstraint", bkSHubLabel)
hubLabelConstraint.MaxTextSize = 31

-- Spin+InfJump Button
