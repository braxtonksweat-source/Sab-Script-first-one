-- LocalScript (place this in StarterPlayerScripts or StarterGui)

local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Create ScreenGui
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "BKsGui"
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.Parent = playerGui

-- ==================== BK's Hub ====================
local BKsHub = Instance.new("Frame")
BKsHub.Name = "BK's Hub"
BKsHub.AnchorPoint = Vector2.new(0.5, 0.5)
BKsHub.BackgroundColor3 = Color3.fromRGB(13, 13, 13)
BKsHub.BorderSizePixel = 0
BKsHub.Position = UDim2.new(0.4516, 0, 0.6203, 0)
BKsHub.Size = UDim2.new(0.1677, 0, 0.4557, 0)
BKsHub.Visible = false
BKsHub.Parent = ScreenGui

local UIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint", BKsHub)
